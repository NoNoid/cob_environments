			<inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" />
			<geometry>
				<box size="${wall_depth} ${size_y} ${size_z_wall2}" />
			</geometry>
		</visual>
		<collision>
			<origin xyz="0 0 0" />
			<geometry>
				<box size="${wall_depth+0.02} ${size_y} ${size_z_wall2}" />
			</geometry>
		</collision>
	</link>

	<gazebo reference="wall_1">
		<material>IPA/Wall2</material>
	</gazebo>

	<joint name="joint_wall_1" type="fixed">
		<origin xyz="${size_x/2} 0 ${size_z_wall2/2}" rpy="0 0 0" />
		<parent link="dummy_link" />
		<child link="wall_1" />
	</joint>

