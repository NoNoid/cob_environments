	xmlns:xacro="http://playerstage.sourceforge.net/gazebo/xmlschema/#interface"
	name="ipa-apartment">

  <property name="size_x" value="6" /> <!--this parameter is not real-->
  <property name="size_y" value="4.3" /> 
  <property name="wall_depth" value="0.06" />
  <property name="size_z_wall1" value="3.20" />
  <property name="size_z_wall2" value="0.5" />

<xacro:macro name="ipa-apartment" params="name">



<!-- dummy_link -->
	<link name="dummy_link">
		<inertial>
			<origin xyz="0 0 0" />
			<mass value="5.0" />
			<inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0" />
		</inertial>
		<visual>
			<origin xyz="0 0 0" />
			<geometry>
				<box size="0.00 0.00 0.00" />
			</geometry>
		</visual>
		<collision>
			<origin xyz="0 0 0" />
			<geometry>
				<box size="0.00 0.00 0.00" />
			</geometry>
		</collision>
	</link>




	<!-- wall_1 -->
	<link name="wall_1">
